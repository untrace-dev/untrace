{
  "dependencies": {
    "@acme/id": "workspace:*",
    "@acme/logger": "workspace:*",
    "@acme/zustand": "workspace:*",
    "@clerk/nextjs": "6.23.3",
    "@supabase/ssr": "0.6.1",
    "@supabase/supabase-js": "2.50.3",
    "@t3-oss/env-core": "0.13.8",
    "@vercel/postgres": "0.10.0",
    "date-fns": "4.1.0",
    "drizzle-orm": "0.44.2",
    "drizzle-seed": "0.3.1",
    "drizzle-zod": "0.8.2",
    "lodash-es": "4.17.21",
    "next": "15.3.5",
    "pg": "8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "zod": "3.25.74",
    "zustand": "5.0.6"
  },
  "devDependencies": {
    "@acme/tsconfig": "workspace:*",
    "@t3-oss/env-core": "0.13.8",
    "@t3-oss/env-nextjs": "0.13.8",
    "@types/lodash-es": "4.17.12",
    "@types/pg": "8.15.4",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "chokidar": "4.0.3",
    "drizzle-kit": "0.31.1",
    "supabase": "2.30.4",
    "tsx": "4.20.3",
    "typescript": "5.8.3",
    "zx": "8.6.1"
  },
  "exports": {
    ".": {
      "default": "./src/index.ts",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "default": "./src/client.ts",
      "types": "./dist/client.d.ts"
    },
    "./schema": {
      "default": "./src/schema.ts",
      "types": "./dist/schema.d.ts"
    },
    "./supabase/cli": {
      "default": "./src/supabase/cli.ts",
      "types": "./dist/supabase/cli.d.ts"
    },
    "./supabase/client": {
      "default": "./src/supabase/client-exports.ts",
      "types": "./dist/supabase/client-exports.d.ts"
    },
    "./supabase/create-channel": {
      "default": "./src/supabase/create-channel.ts",
      "types": "./dist/supabase/create-channel.d.ts"
    },
    "./supabase/rcs": {
      "default": "./src/supabase/server.rcs.ts",
      "types": "./dist/supabase/server.rcs.d.ts"
    },
    "./supabase/server": {
      "default": "./src/supabase/server-exports.ts",
      "types": "./dist/supabase/server-exports.d.ts"
    },
    "./supabase/types": {
      "default": "./src/supabase/types.generated.ts",
      "types": "./dist/supabase/types.generated.d.ts"
    }
  },
  "license": "MIT",
  "name": "@acme/db",
  "private": true,
  "scripts": {
    "build": "tsc",
    "build.todo": "bun gen-supabase-types && tsc",
    "clean": "rm -rf .turbo node_modules dist",
    "create-supabase-policies": "tsx scripts/create-supabase-policies.ts",
    "dev": "tsx ./scripts/watch.ts",
    "enable-supabase-realtime": "tsx scripts/enable-supabase-realtime.ts",
    "gen-migration": "drizzle-kit generate",
    "gen-supabase-types": "tsx ./scripts/generate-supabase-types.ts",
    "migrate": "drizzle-kit migrate && bun gen-supabase-types",
    "push": "drizzle-kit push && bun gen-supabase-types && bun create-supabase-policies && bun enable-supabase-realtime",
    "seed": "tsx src/seed.ts",
    "studio": "drizzle-kit studio",
    "supabase:local": "supabase start",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "version": "0.1.0"
}
