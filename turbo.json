{
  "$schema": "https://turborepo.org/schema.json",
  "globalEnv": [
    "DATABASE_URL",
    "PORT",
    "CLERK_SECRET_KEY",
    "POSTGRES_URL",
    "POSTGRES_PRISMA_URL",
    "POSTGRES_URL_NON_POOLING",
    "POSTGRES_USER",
    "POSTGRES_HOST",
    "POSTGRES_PASSWORD",
    "POSTGRES_DATABASE",
    "SUPABASE_ANON_KEY",
    "SUPABASE_URL",
    "SUPABASE_SERVICE_ROLE_KEY",
    "SUPABASE_JWT_SECRET",
    "CLERK_WEBHOOK_SECRET",
    "CLERK_ISSUER_URL",
    "OPENAI_API_KEY",
    "POSTHOG_KEY",
    "POSTHOG_HOST",
    "KV_URL",
    "KV_REST_API_READ_ONLY_TOKEN",
    "REDIS_URL",
    "KV_REST_API_TOKEN",
    "KV_REST_API_URL",
    "NEXT_PUBLIC_API_URL",
    "STRIPE_SECRET_KEY",
    "STRIPE_PUBLISHABLE_KEY",
    "STRIPE_WEBHOOK_SECRET",
    "NEXT_PUBLIC_SUPABASE_URL",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY",
    "SUPABASE_PROJECT_ID",
    "NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY",
    "NEXT_PUBLIC_POSTHOG_KEY",
    "NEXT_PUBLIC_POSTHOG_HOST",
    "NEXT_PUBLIC_APP_ENV",
    "SENTRY_ORG",
    "SENTRY_PROJECT",
    "SENTRY_AUTH_TOKEN",
    "COMMITTER_TOKEN",
    "APPLE_TEAM_ID",
    "APPLE_ID",
    "APPLE_PASSWORD",
    "MACOS_CERTIFICATE",
    "MACOS_CERTIFICATE_PWD",
    "MACOS_IDENTITY",
    "POSTHOG_PERSONAL_API_KEY",
    "NPM_TOKEN",
    "TURBO_TOKEN",
    "TURBO_TEAM"
  ],
  "globalPassThroughEnv": [
    "NODE_ENV",
    "CI",
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_URL",
    "npm_lifecycle_event"
  ],
  "tasks": {
    "//#clean": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "next-env.d.ts",
        ".expo/**",
        ".output/**",
        ".vercel/output/**",
        "dist/**"
      ]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "format": {
      "dependsOn": ["^topo", "^build"]
    },
    "gen-migration": {
      "cache": false,
      "interactive": true
    },
    "migrate": {
      "cache": false,
      "interactive": true
    },
    "push": {
      "cache": false,
      "interactive": true
    },
    "seed": {
      "cache": false,
      "interactive": true
    },
    "studio": {
      "cache": false,
      "persistent": true
    },
    "topo": {
      "dependsOn": ["^topo"]
    },
    "typecheck": {
      "dependsOn": ["^topo", "^build"],
      "outputs": ["node_modules/.cache/tsbuildinfo.json"]
    },
    "ui-add": {
      "cache": false,
      "interactive": true
    }
  },
  "ui": "tui"
}
